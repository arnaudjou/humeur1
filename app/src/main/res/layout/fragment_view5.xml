<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/fragment_view5_rootview"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.controller.fragment.fragments.View5Fragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageView
        android:id="@+id/View5"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:clickable="true"
        android:scaleType="fitCenter"
        android:background="@color/faded_red"
        android:src="@drawable/ic_android_sad"/>

    <ImageButton
        android:id="@+id/HistoryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        android:clickable="true"
        android:src="@drawable/ic_history_black" />

    <ImageButton
        android:id="@+id/CommentaryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:clickable="true"
        android:src="@drawable/ic_note_add_black" />

</RelativeLayout>