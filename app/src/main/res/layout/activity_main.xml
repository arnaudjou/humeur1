<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:name="com.example.nono.humeur.controller.fragment.View1Fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.nono.humeur.controller.controller.fragment.VerticalViewPager
        android:id="@+id/activity_main_viewpager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp" />

    <ImageButton
        android:id="@+id/HistoryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        android:clickable="true"
        android:focusable="true"
        android:src="@drawable/ic_history_black" />

    <ImageButton
        android:id="@+id/CommentaryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:clickable="true"
        android:src="@drawable/ic_note_add_black"
        android:focusable="true"
        android:contentDescription="TODO" />


</RelativeLayout>