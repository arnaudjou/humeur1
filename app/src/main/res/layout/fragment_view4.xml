<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.controller.fragment.fragments.View4Fragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageButton
        android:id="@+id/View4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitCenter"
        android:background="@color/warm_grey"
        android:onClick="onClick"
        android:clickable="true"
        android:src="@drawable/ic_android_disappointed"/>

    <ImageButton
        android:id="@+id/HistoryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:onClick="onClick"
        android:clickable="true"
        android:src="@drawable/ic_history_black"
        android:focusable="true" />

    <ImageButton
        android:id="@+id/CommentaryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:onClick="onClick"
        android:clickable="true"
        android:src="@drawable/ic_note_add_black" />


</LinearLayout>
